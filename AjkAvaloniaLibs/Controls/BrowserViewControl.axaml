<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AjkAvaloniaLibs.Controls.BrowserViewControl">
	<DockPanel>
		<Menu DockPanel.Dock="Top" Name="Menu" FontSize="12">
			<MenuItem Header="_Tool" Name="MenuItem_Tool">
				<MenuItem Header="_OpenDevTools" Name="MenuItem_OpenDevTools" Click="MenuItem_OpenDevTools_Click"/>
			</MenuItem>
		</Menu>
		<TextBox x:Name="addressTextBox" DockPanel.Dock="Top" />
		<Decorator x:Name="browserWrapper"/>
	</DockPanel>
</UserControl>
